{
  "name": "getolympus/olympus-zeus-core",
  "type": "framework",
  "description": "Olympus Zeus framework core system used to make all your WordPress plugins and themes developments easier and efficient.",
  "keywords": ["olympus", "zeus", "wordpress", "framework", "php"],
  "homepage": "https://github.com/GetOlympus/Zeus-Core",
  "license": "MIT",
  "authors": [
    {
      "name": "Achraf Chouk",
      "homepage": "https://github.com/crewstyle"
    }
  ],
  "autoload": {
    "psr-4": {
      "GetOlympus\\Zeus\\": "src/Zeus/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "behat/transliterator": "^1.1@dev",
    "getolympus/olympus-background-field": "~0.0.1",
    "getolympus/olympus-checkbox-field": "~0.0.1",
    "getolympus/olympus-code-field": "~0.0.1",
    "getolympus/olympus-color-field": "~0.0.1",
    "getolympus/olympus-date-field": "~0.0.1",
    "getolympus/olympus-file-field": "~0.0.1",
    "getolympus/olympus-font-field": "~0.0.1",
    "getolympus/olympus-hidden-field": "~0.0.1",
    "getolympus/olympus-html-field": "~0.0.1",
    "getolympus/olympus-link-field": "~0.0.1",
    "getolympus/olympus-map-field": "~0.0.1",
    "getolympus/olympus-multiselect-field": "~0.0.1",
    "getolympus/olympus-radio-field": "~0.0.1",
    "getolympus/olympus-rte-field": "~0.0.1",
    "getolympus/olympus-select-field": "~0.0.1",
    "getolympus/olympus-social-field": "~0.0.1",
    "getolympus/olympus-text-field": "~0.0.1",
    "getolympus/olympus-textarea-field": "~0.0.1",
    "getolympus/olympus-toggle-field": "~0.0.1",
    "getolympus/olympus-upload-field": "~0.0.1",
    "getolympus/olympus-wordpress-field": "~0.0.1",
    "league/container": "^2.2",
    "ruflin/Elastica": ">=3.0",
    "symfony/class-loader": "^3.1",
    "symfony/config": "^3.1",
    "symfony/http-foundation": "^3.0",
    "symfony/translation": "^3.0",
    "symfony/yaml": "^3.0",
    "twig/twig": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "squizlabs/php_codesniffer": "2.*"
  },
  "scripts": {
    "test": [
      "phpunit",
      "vendor/bin/phpcs --standard=phpcs.xml ."
    ]
  }
}
