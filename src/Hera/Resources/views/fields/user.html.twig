<tr id="{% block id %}{% endblock %}-content" class="olh-field {% block class %}{% endblock %}">
    <th>
        {% block header %}{% endblock %}
        {% block label %}{% endblock %}
        {% block overall %}{% endblock %}
    </th>
    <td>
        {% block field %}{% endblock %}
        <p class="olh-description">{% block description %}{% endblock %}</p>
        {% block footer %}{% endblock %}
    </td>
</tr>
