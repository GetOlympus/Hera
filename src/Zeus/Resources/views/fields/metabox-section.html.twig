<p id="{% block id %}{% endblock %}-field" class="zeus-field {% block class %}{% endblock %}">
    {% block header %}{% endblock %}
    {% block overall %}{% endblock %}

    <label for="{% block id %}{% endblock %}">
        {% block label %}{% endblock %}
    </label>

    {% block field %}{% endblock %}
    {% block footer %}{% endblock %}
</p>

{% block description %}{% endblock %}
