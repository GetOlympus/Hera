<tr id="{% block id %}{% endblock %}-content" class="form-field olh-field term-{% block class %}{% endblock %}-wrap">
    <th scope="row">
        {% block header %}{% endblock %}

        {% block label %}{% endblock %}
        {% block overall %}{% endblock %}
    </th>

    <td>
        {% block field %}{% endblock %}
        <p class="olh-description description">{% block description %}{% endblock %}</p>

        {% block footer %}{% endblock %}
    </td>
</tr>
